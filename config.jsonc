{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "source": "$2\u0020\u0020\u0020\u0020\u0020██\n\u0020\u0020\u0020\u0020\u0020██\n\u0020\u0020\u0020\u0020\u0020██\n\u0020\u0020\u0020\u0020\u0020██\n\u0020\u0020\u0020\u0020\u0020██\n\u0020\u0020████████\n\u0020\u0020\u0020\u0020\u0020██",
        "type": "data",
        "padding": {
            "top": 5,
            "left": 1,
            "right": 2,
        },
    },
    "display": {
          "separator": "  ▶  ",
          "color": {
          "separator": "white",
          }
    },
    "modules":[
         {
            "type": "os",
            "key": " ┏ OS",
            "keyColor": "31",
        },
        {
            "type": "kernel",
            "key": " ┣  ",
            "keyColor": "31",
        },
        {
            "type": "packages",
            "format": "{} (pacman)",
            "key": " ┣ 󰏖 ",
            "keyColor": "31",  
        },
        {
            "type": "shell",
            "key": " ┗  ",
            "keyColor": "31", 
        },
        "break",
        {
            "type": "wm",
            "key": " ┏ WM",
            "keyColor": "32", 
        },
        {
            "type": "monitor",
            "key": " ┣  ",
            "format": "{1} ({2} x {3}) @ {11}Hz",
            "keyColor": "32",
        },
        {
            "type": "wmtheme",
            "key": " ┣ 󰉼 ",
            "keyColor": "32",
        },
        {
            "type": "icons",
            "key": " ┣ 󰀻 ",
            "keyColor": "32",
        },
        {
            "type": "cursor",
            "key": " ┣  ",
            "keyColor": "32", 
        },
        {
            "type": "terminal",
            "key": " ┣  ",
            "keyColor": "32",
        },
        {
            "type": "terminalfont",
