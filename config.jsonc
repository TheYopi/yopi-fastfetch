            "keyColor": "33",
        },
        {
            "type": "gpu",
            "format": "{1} {2}",            
            "key": " ┣ 󰢮 ",
            "keyColor": "33",
        },
        {
            "type": "memory",
            "key": " ┣  ",
            "keyColor": "33",
        },
        {
            "type": "disk",
            "key": " ┣ 󰋊 ",
            "format": "{10} - {1} / {2} ({3})",
            "keyColor": "33",
        },
        {
            "type": "uptime",
            "key": " ┗ ⧗ ",
            "keyColor": "33",
        },
    ]
